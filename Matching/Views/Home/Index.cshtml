@model IEnumerable<Identity.Models.AppUser>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <style>

        .carousel-control-next-icon,
        .carousel-control-prev-icon {
         filter: invert(1);
         }

         #image{
             margin-left:170px;
             max-width:300px;
             height:400px;
             border-radius:40px;
         }
         
         #demo{
             margin:auto;
             width:50%;
         }


    </style>

    <div class="text-center">

        @foreach (var user in @Model)
        {
            <h2>@user.UserName</h2>
        
        }
           <!-- Carousel -->
        <div id="demo" class="carousel slide">

          <!-- The slideshow/carousel -->
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img src="~/photo1.jpg" alt="Los Angeles" class="d-block" id="image">
            </div>
            <div class="carousel-item">
              <img src="~/photo3.jpg" alt="Chicago" class="d-block" id="image">
            </div>
            <div class="carousel-item">
              <img src="~/photo2.jpg" alt="Chicago" class="d-block" id="image">
            </div>
            <div class="carousel-item">
              <img src="~/photo4.jpg" alt="Chicago" class="d-block" id="image">
            </div>
             <div class="carousel-item">
              <img src="~/photo5.jpg" alt="Chicago" class="d-block" id="image">
            </div>
            <div class="carousel-item">
              <img src="~/photo6.jpg" alt="Chicago" class="d-block" id="image">
            </div>
             <div class="carousel-item">
              <img src="~/photo7.jpg" alt="Chicago" class="d-block" id="image">
            </div>
             <div class="carousel-item">
              <img src="~/photo8.jpg" alt="Chicago" class="d-block" id="image">
            </div>
       
          </div>
  
          <!-- Left and right controls/icons -->
          <button class="carousel-control-prev" type="button" data-bs-target="#demo" data-bs-slide="prev" >
            <span class="carousel-control-prev-icon"></span>
          </button>
          <button class="carousel-control-next" type="button" data-bs-target="#demo" data-bs-slide="next">
            <span class="carousel-control-next-icon"></span>
          </button>
        </div>
   
    </div>
